##############
# Github.com #
##############
Host github.intel.com
        Protocol 2
        User esande2x
        ServerAliveInterval 30
        IdentityFile /home/esande2x/.ssh/internal.key

Host github.com
        Protocol 2
        User git
        ServerAliveInterval 30
        IdentityFile /home/esande2x/.ssh/personal.key
        ProxyCommand nc -x proxy.jf.intel.com:1080 %h %p



##########
# 01.org #
##########
 
Host a1.01.org a2.01.org 
        Protocol 2
        User esande2x
        ServerAliveInterval 30
        IdentityFile /home/esande2x/.ssh/01.org.key
        ProxyCommand nc -x proxy.jf.intel.com:1080 %h %p

Host *.vlan*.01.org
        Protocol 2
        User esande2x
        ServerAliveInterval 30
        IdentityFile /home/esande2x/.ssh/01.org.key
        ProxyCommand ssh a1.01.org -W %h:%p

Host a3.vlan14.01.org 
        Protocol 2
        User esande2x
        ServerAliveInterval 30
        IdentityFile /home/esande2x/.ssh/01.org.key
        ProxyCommand ssh a3.01.org -W %h:%p
 
